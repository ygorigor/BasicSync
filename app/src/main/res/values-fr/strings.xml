    <string name="dialog_import_password_title">Importation de config</string>
    <string name="dialog_import_password_message">Le fichier zip sélectionné est chiffré. Entrez le mot de passe de déchiffrement pour l\'importer.</string>
    <string name="dialog_import_password_hint">Mot de passe de déchiffrement</string>
    <string name="dialog_export_password_title">Exportation de config</string>
    <string name="dialog_export_password_message">Entrez un mot de passe pour chiffrer le fichier zip exporté ou laissez vide pour le stocker non chiffré.</string>
    <string name="dialog_export_password_hint">Mot de passe de chiffrement</string>
    <string name="dialog_export_password_confirm_hint">Confirmer le mot de passe</string>
