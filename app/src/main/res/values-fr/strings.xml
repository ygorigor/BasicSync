<resources>
    <!-- Preference headers -->
    <string name="pref_header_permissions">Autorisations</string>
    <string name="pref_header_configuration">Configuration</string>
    <string name="pref_header_run_conditions">Conditions d\'exécution</string>
    <string name="pref_header_about">À propos</string>
    <string name="pref_header_debug">Débogage</string>

    <!-- Main preferences -->
    <string name="pref_inhibit_battery_opt_name">Désactiver l\'optimisation de la batterie</string>
    <string name="pref_inhibit_battery_opt_desc">Nécessaire pour fonctionner de manière fiable en arrière-plan.</string>
    <string name="pref_allow_notifications_name">Autoriser les notifications</string>
    <string name="pref_allow_notifications_desc">Nécessaire pour fonctionner de manière fiable en arrière-plan et pour démarrer et arrêter Syncthing.</string>
    <string name="pref_local_storage_access_name">Autoriser l\'accès au stockage local</string>
    <string name="pref_local_storage_access_desc">Nécessaire pour que Syncthing accède au stockage local sous <tt>/sdcard</tt>.</string>
    <string name="pref_disable_app_hibernation_name">Désactiver la mise en veille de l\'application</string>
    <string name="pref_disable_app_hibernation_desc">Nécessaire pour empêcher Android de supprimer automatiquement les autorisations de l\'application après un certain temps.</string>
    <string name="pref_open_web_ui_name">Ouvrir l\'interface web</string>
    <string name="pref_open_web_ui_desc">Ouvrir l\'interface de configuration web de Syncthing.</string>
    <string name="pref_import_configuration_name">Importer la configuration</string>
    <string name="pref_import_configuration_desc">Importer un fichier zip de configuration Syncthing existant.</string>
    <string name="pref_export_configuration_name">Exporter la configuration</string>
    <string name="pref_export_configuration_desc">Exporter la configuration Syncthing actuelle vers un fichier zip.</string>
    <string name="pref_service_status_name">État de Syncthing</string>
    <string name="pref_auto_mode_name">Mode automatique</string>
    <string name="pref_auto_mode_desc">Démarrer et arrêter automatiquement en fonction des conditions spécifiées.</string>
    <string name="pref_network_conditions_name">Conditions réseau</string>
    <string name="pref_network_conditions_desc">Exécuter lors de la connexion à certains types de réseau ou réseaux Wi-Fi.</string>
    <string name="pref_run_on_battery_name">Fonctionner sur batterie</string>
    <string name="pref_run_on_battery_desc">Fonctionner lorsque l\'appareil est sur batterie et que le niveau est d\'au moins %1$d%%. Syncthing fonctionne toujours lorsqu\'il est branché.</string>
    <string name="pref_respect_battery_saver_name">Respecter le mode économie de batterie</string>
    <string name="pref_respect_battery_saver_desc">Ne fonctionner que lorsque le mode économie de batterie d\'Android est désactivé.</string>
    <string name="pref_respect_auto_sync_data_name">Respecter le paramètre de synchronisation automatique</string>
    <string name="pref_respect_auto_sync_data_desc">Ne fonctionner que lorsque le paramètre de synchronisation automatique d\'Android est activé.</string>
    <string name="pref_sync_schedule_name">Synchronisation planifiée</string>
    <string name="pref_sync_schedule_desc">Synchroniser pendant %2$s sur chaque cycle de %1$s.</string>
    <string name="pref_keep_alive_name">Maintenir Syncthing actif</string>
    <string name="pref_keep_alive_desc">Lorsque Syncthing ne doit pas fonctionner, le mettre en pause plutôt que de l\'arrêter. Cela évite une baisse potentielle des performances lors de l\'analyse de grands dossiers au démarrage.</string>
    <string name="pref_version_name">Version</string>
    <string name="pref_save_logs_name">Enregistrer les journaux</string>
    <string name="pref_save_logs_desc">Enregistrer les journaux logcat dans un fichier.</string>

    <!-- Network condition preferences -->
    <string name="pref_header_allowed_network_types">Types de réseau autorisés</string>
    <string name="pref_header_allowed_wifi_networks">Réseaux Wi-Fi autorisés</string>
    <string name="pref_header_available_wifi_networks">Réseaux Wi-Fi disponibles</string>
